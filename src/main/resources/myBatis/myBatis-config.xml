<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.zest.web.client.model.LectureVO"  alias="lectureVO" />
		<typeAlias type="com.zest.web.client.model.CategoryVO" alias="CategoryVO" />
		<typeAlias type="com.zest.web.client.model.ClientVO" alias="ClientVO" />	
		<typeAlias type="com.zest.web.client.model.Tutor_PropVO" alias="Tutor_PropVO" />	
		<typeAlias type="com.zest.web.client.model.TuTorVO" alias="TutorVO" />
	</typeAliases>
	
    <!--Sql Mapper 설정-->
    <mappers>
        <!--위치값을 줄때 루트 폴터는 resources 폴더이다.-->
        <mapper resource="myBatis/test/test.xml"></mapper>
        <mapper resource="myBatis/category/category-sqlMapping.xml"/>
        <mapper resource="myBatis/category/lecture-sqlMapping.xml" />        
        <!-- 클라이언트 -->
        <mapper resource="myBatis/client/client-sqlMapping.xml" />
        
        <!-- 튜터  신청-->
        <mapper resource="myBatis/tutor/tutor_prop-sqlMapping.xml" />
        <!-- 튜터 -->
        <mapper resource="myBatis/tutor/tutor-sqlMapping.xml" />
    </mappers>
</configuration>