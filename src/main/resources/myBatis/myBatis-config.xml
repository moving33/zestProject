<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.zest.web.client.model.LectureVO"  alias="lectureVO" />
		<typeAlias type="com.zest.web.client.model.CategoryVO" alias="CategoryVO" />
		<typeAlias type="com.zest.web.client.model.ClientVO" alias="ClientVO" />	
		<typeAlias type="com.zest.web.client.model.Tutor_PropVO" alias="Tutor_PropVO" />	
		<typeAlias type="com.zest.web.client.model.TuTorVO" alias="TutorVO" />
		<typeAlias type="com.zest.web.admin.model.AdminVO" alias="AdminVO" />
		<typeAlias type="com.zest.web.client.model.ClassDetailVO" alias="DetailVO" />
		<typeAlias type="com.zest.web.client.model.CategoryPageVO" alias="CategoryPageVO"/>		
		<typeAlias type="com.zest.web.client.model.ClassDetail_TutorVO" alias="ClassDetail_TutorVO"/>
		<typeAlias type="com.zest.web.client.model.TalentVO" alias="TalentVO" />
		<typeAlias type="com.zest.web.client.model.Talent_contentVO" alias="TC_VO" />
		<typeAlias type="com.zest.web.client.model.Talent_notOneDayTime" alias="TT_notTimeVO" />
		<typeAlias type="com.zest.web.client.model.Talent_info" alias="TI_VO" />
		<typeAlias type="com.zest.web.client.model.ZoneVO" alias="ZoneVO" />
		<!-- 원데이 타임 vo -->
		<typeAlias type="com.zest.web.client.model.Talent_OneDayTimeVO" alias="TT_TimeVO" />
		<typeAlias type="com.zest.web.client.model.ReviewVO" alias="ReviewVO" />
	</typeAliases>
	
    <!--Sql Mapper 설정-->
    <mappers>
        <!--위치값을 줄때 루트 폴터는 resources 폴더이다.-->
        <mapper resource="myBatis/test/test.xml"></mapper>
        <mapper resource="myBatis/category/category-sqlMapping.xml"/>
        <mapper resource="myBatis/category/categoryPage-sqlMapping.xml"/>
        <mapper resource="myBatis/category/lecture-sqlMapping.xml" />        
        <!-- 클라이언트 -->
        <mapper resource="myBatis/client/client-sqlMapping.xml" />
        
        <!-- 튜터  신청-->
        <mapper resource="myBatis/tutor/tutor_prop-sqlMapping.xml" />
        <!-- 튜터 -->
        <mapper resource="myBatis/tutor/tutor-sqlMapping.xml" />
        <!-- 관리자 -->
        <mapper resource="myBatis/admin/admin-sqlMapping.xml" />

        
        

        <!-- 강의 -->
        <mapper resource="myBatis/talent/talent-sqlMapping.xml" />
        <!-- 강의 내용 -->
        <mapper resource="myBatis/talent/talent_content-sqlMapping.xml" />
        <!-- 강의 정보 -->
         <mapper resource="myBatis/talent/talent_Info-sqlMapping.xml" />
         <!-- 강의 notOneday 시간 -->
         <mapper resource="myBatis/talent/talent_notOnedayTime-sqlMapping.xml" />
         <!-- 강의 Oneday 시간 -->
         <mapper resource="myBatis/talent/talent_OnedayTime-sqlMapping.xml" />
         <!-- 지역  -->
         <mapper resource="myBatis/zone/zone-sqlMapping.xml" />
         
        <!-- 강의상세 -->
        <mapper resource="myBatis/category/classDetail-sqlMapping.xml" />
        <!-- 리뷰 -->
        <mapper resource="myBatis/client/review-sqlMapping.xml" />
    </mappers>
</configuration>